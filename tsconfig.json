{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "src/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/**/*.tsx",
    "src/**/**/*.ts"
  ]
}
